<div class="field is-horizontal">
                
    <div class="field-body">
        <div class="field">
            <div class="columns">
                <div class="column">

                    <p class="control has-icons-left">
                        <input class="input" type="text" name="IDContrato" value="<%= formData?.IDContrato %>" autocomplete="off" placeholder="Número de Contrato" required>
                        <span class="icon is-small is-left">
                            <i class="fas fa-hashtag"></i>
                        </span>
                    </p>
                    <% if(error){%>
                        <div class="tag">
                            <strong><%=error%></strong>
                        </div>
                    <%}%>
                </div>
                <div class="column is-four-fifths">
                    <p class="control has-icons-left">
                        <input class="input" type="date" name="fecha_venta" value="<%= formData?.fecha_venta %>" autocomplete="off" placeholder="Fecha de Contrato">
                        <span class="icon is-small is-left">
                            <i class="fas fa-calendar"></i>
                        </span>
                        <p class="help">Fecha de Contrato</p>
                    </p>
                    <p></p>
                </div>
                
            </div>
        </div>
    </div>
</div>

<div class="field is-horizontal">
    
    <div class="field-body">
        <div class="field">
            <p class="control is-expanded has-icons-left">
                <input class="input" type="text" name="nombre" value="<%= formData?.nombre %>" autocomplete="off" placeholder="Nombre">
                <span class="icon is-small is-left">
                    <i class="fas fa-user"></i>
                </span>
            </p>
        </div>
        <div class="field">
            <p class="control is-expanded has-icons-left">
                <input class="input" type="text" name="apellido" value="<%= formData?.apellido %>" autocomplete="off" placeholder="Apellido">
                <span class="icon is-small is-left">
                    <i class="fas fa-user"></i>
                </span>
            </p>
        </div>
    </div>
</div>

<div class="field is-horizontal">
    <div class="field-body">
        <div class="field">
            <div class="field has-addons">
                <p class="control is-expanded has-icons-left">
                    <input class="input" type="tel" name="telefono" value="<%= formData?.telefono %>" autocomplete="off" placeholder="Número Telefónico">
                    <span class="icon is-small is-left">
                        <i class="fas fa-phone"></i>
                    </span>
                </p>
            </div>
        </div>
        <div class="field">
            <p class="control is-expanded has-icons-left">
                <input class="input" type="date" autocomplete="off" name="fecha_entrega" value="<%= formData?.fecha_entrega %>">
                <span class="icon is-small is-left">
                    <i class="fas fa-calendar"></i>
                </span>
                
            </p>
            <p class="help">Fecha de Entrega</p>
        </div>
        <div class="field">
            <p class="control is-expanded has-icons-left">
                <input class="input" type="date" autocomplete="off" name="fecha_recibido" value="<%= formData?.fecha_recibido %>">
                <span class="icon is-small is-left">
                    <i class="fas fa-calendar"></i>
                </span>
                
            </p>
            <p class="help">Fecha Recibido</p>
        </div>
        <div class="field is-small">
            <p class="control has-icons-left">
                <input 
                    class="input" 
                    type="text" 
                    id="numero_armazones" 
                    name="numero_armazones"
                    autocomplete="off" 
                    placeholder="Número de armazones"
                    value="<%= formData?.numero_armazones %>"
                    required
                >
                <span class="icon is-small is-left">
                    <i class="fas fa-hashtag"></i>
                </span>
            </p>
        </div>
    </div>
</div>